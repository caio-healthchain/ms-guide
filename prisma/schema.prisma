}

model documents {
  id           String           @id
  createdAt    DateTime         @default(now())
  updatedAt    DateTime
  title        String
  filename     String
  originalName String
  mimeType     String
  size         Int
  category     DocumentCategory
  subcategory  String?
  tags         String[]
