generator client {
  provider = "prisma-client-js"
  output   = "../node_modules/.prisma/client"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model ai_agents {
  id          String   @id
  createdAt   DateTime @default(now())
  updatedAt   DateTime
  name        String
  description String?
  type        String
  config      Json
  model       String
  version     String   @default("1.0")
  isActive    Boolean  @default(true)
  isTraining  Boolean  @default(false)
  tags        String[]

  @@index([isActive])
  @@index([type])
}

model audit_logs {
  id               String            @id
  createdAt        DateTime          @default(now())
  entityId         String
  userId           String
  userName         String
  userRole         String
  patientId        String?
  procedureId      String?
  newValues        Json?
  oldValues        Json?
  billingAccountId String?
  changes          Json?
  ipAddress        String?
  sessionId        String?
  userAgent        String?
  entity           AuditLogEntity
  action           AuditLogAction
  billing_accounts billing_accounts? @relation(fields: [billingAccountId], references: [id])
  patients         patients?         @relation(fields: [patientId], references: [id])
  procedures       procedures?       @relation(fields: [procedureId], references: [id])

  @@index([action])
  @@index([createdAt])
  @@index([entityId])
  @@index([entity])
  @@index([patientId])
  @@index([userId])
}

/// This model or at least one of its fields has comments in the database, and requires an additional setup for migrations: Read more: https://pris.ly/d/database-comments
model auditoria_validacoes {
  id              String          @id
  createdAt       DateTime        @default(now())
  updatedAt       DateTime
  guiaId          Int
  procedimentoId  Int
  tipoValidacao   TipoValidacao
  status          StatusValidacao
  mensagem        String?
  valorEsperado   Decimal?        @db.Decimal(10, 2)
  valorEncontrado Decimal?        @db.Decimal(10, 2)
  diferenca       Decimal?        @db.Decimal(10, 2)
  metadata        Json?
  auditorId       String?
  dataValidacao   DateTime?
  observacoes     String?

  @@unique([guiaId, procedimentoId, tipoValidacao], map: "auditoria_validacoes_unique_idx")
  @@index([guiaId])
  @@index([procedimentoId])
  @@index([status])
  @@index([tipoValidacao])
}

model billing_accounts {
  id              String          @id
  patientId       String
  accountNumber   String          @unique
  totalAmount     Decimal         @db.Decimal(10, 2)
  paidAmount      Decimal         @default(0) @db.Decimal(10, 2)
  remainingAmount Decimal         @db.Decimal(10, 2)
  status          BillingStatus
  dueDate         DateTime
  createdAt       DateTime        @default(now())
  updatedAt       DateTime
  audit_logs      audit_logs[]
  patients        patients        @relation(fields: [patientId], references: [id])
  billing_items   billing_items[]

  @@index([accountNumber])
  @@index([patientId])
  @@index([status])
}

model billing_items {
  id               String           @id
  billingAccountId String
  patientId        String
  procedureId      String?
  description      String
  quantity         Int              @default(1)
  unitPrice        Decimal          @db.Decimal(10, 2)
  totalPrice       Decimal          @db.Decimal(10, 2)
  discount         Decimal          @default(0) @db.Decimal(10, 2)
  status           BillingStatus
  createdAt        DateTime         @default(now())
  updatedAt        DateTime
  billing_accounts billing_accounts @relation(fields: [billingAccountId], references: [id])
  patients         patients         @relation(fields: [patientId], references: [id])
  procedures       procedures?      @relation(fields: [procedureId], references: [id])

  @@index([billingAccountId])
  @@index([patientId])
  @@index([procedureId])
}

model business_rules {
  id          String           @id
  createdAt   DateTime         @default(now())
  updatedAt   DateTime
  name        String
  description String?
  code        String           @unique
  type        BusinessRuleType
  status      RuleStatus
  priority    Int              @default(0)
  conditions  Json
  actions     Json
  version     String           @default("1.0")
  isActive    Boolean          @default(true)
  tags        String[]

  @@index([code])
  @@index([isActive])
  @@index([status])
  @@index([type])
}

/// This model or at least one of its fields has comments in the database, and requires an additional setup for migrations: Read more: https://pris.ly/d/database-comments
model contrato {
  id          Int       @id @default(autoincrement())
  numero      String    @unique @db.VarChar(255)
  operadoraId String    @db.VarChar(255)
  status      String    @db.VarChar(50)
  dataInicio  DateTime  @db.Timestamp(6)
  dataFim     DateTime  @db.Timestamp(6)
  createdAt   DateTime? @default(now()) @db.Timestamp(6)
  updatedAt   DateTime? @default(now()) @db.Timestamp(6)

  @@index([dataFim])
  @@index([dataInicio])
  @@index([numero])
  @@index([operadoraId])
  @@index([status])
}

/// This model or at least one of its fields has comments in the database, and requires an additional setup for migrations: Read more: https://pris.ly/d/database-comments
model contrato_itens {
  id                          String                        @id
  createdAt                   DateTime                      @default(now())
  updatedAt                   DateTime
  contratoId                  String
  codigoTUSS                  String
  descricao                   String?
  valorContratado             Decimal                       @db.Decimal(10, 2)
  estaNoPacote                Boolean                       @default(false)
  requerAutorizacao           Boolean                       @default(false)
  observacoes                 String?
  tipoItem                    String?
  valorMaximo                 Decimal?                      @db.Decimal(10, 2)
  quantidadeMaxima            Int?
  contratos                   contratos                     @relation(fields: [contratoId], references: [id], onDelete: Cascade)
  contrato_materiais_inclusos contrato_materiais_inclusos[]

  @@unique([contratoId, codigoTUSS], map: "contrato_itens_contratoId_codigoTUSS_idx")
  @@unique([contratoId, codigoTUSS])
  @@index([codigoTUSS])
  @@index([contratoId])
  @@index([tipoItem])
}

model contrato_materiais_inclusos {
  id                String         @id
  createdAt         DateTime       @default(now())
  updatedAt         DateTime
  contratoItemId    String
  descricaoMaterial String         @db.VarChar(500)
  quantidade        Int            @default(1)
  obrigatorio       Boolean        @default(true)
  contrato_itens    contrato_itens @relation(fields: [contratoItemId], references: [id], onDelete: Cascade)

  @@index([contratoItemId])
}

model contratos {
  id                  String           @id
  createdAt           DateTime         @default(now())
  updatedAt           DateTime
  numero              String           @unique
  operadoraId         String
  dataInicio          DateTime
  dataFim             DateTime?
  valorMensal         Decimal?         @db.Decimal(12, 2)
  status              ContratoStatus   @default(ATIVO)
  tipoPagamento       String?
  prazoMedioPagamento Int?
  observacoes         String?
  contrato_itens      contrato_itens[]
  operadoras          operadoras       @relation(fields: [operadoraId], references: [id])

  @@index([dataFim])
  @@index([dataInicio])
  @@index([operadoraId])
  @@index([status])
}

model dialogues {
  id         String   @id
  createdAt  DateTime @default(now())
  updatedAt  DateTime
  patientId  String
  title      String
